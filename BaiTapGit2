void phepToanCoBan(int a, int b){
	cout<<"a+b= "<<a+b<<endl;
	cout<<"a-b= "<<a-b<<endl;
	cout<<"a*b= "<<a*b<<endl;
	if(b==0) cout<<"Khong the chia cho 0"<<endl;
	else 	cout<<"a/b= "<<a/b<<endl;
}

